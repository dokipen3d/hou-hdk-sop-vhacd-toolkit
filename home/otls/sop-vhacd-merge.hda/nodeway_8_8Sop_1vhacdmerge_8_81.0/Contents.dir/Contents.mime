Content-Disposition: attachment; filename="vhacdmerge1/update_B_attributes.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// ---------------------------------------------------------------------
// VARIABLES
// ---------------------------------------------------------------------
string  _opname = chs(\"opname\");
int     success;

// ---------------------------------------------------------------------
// HELPERS
// ---------------------------------------------------------------------
#define PRINT(val) printf(\"%d\\n\", val)
#define WTF() printf(\"WTF? line: %d in: \\\"%s\\\"\\n\", __LINE__, _opname)
#define NOSUCCESS() printf(\"wasSuccess == %d in: \\\"%s\\\"\\n\", wasSuccess, _opname)

void UpdateAttribute(string attribname, maxattribname; int primitive)
{
    int wasSuccess;
    int maxValue = detailattrib(1, maxattribname, 0, wasSuccess);
    if (wasSuccess)
    {
        int newValue = primattrib(geoself(), attribname, primitive, wasSuccess);
        if (wasSuccess)
        {
            newValue += maxValue + 1;    
            setprimattrib(geoself(), attribname, primitive, newValue);
        }
    }
}

// ---------------------------------------------------------------------
// MAIN
// ---------------------------------------------------------------------

// update bundle_id
success = hasprimattrib(geoself(), \"bundle_id\");
if (success) UpdateAttribute(\"bundle_id\", \"max_bundle_id\", i@primnum);

// update hull_id
success = hasprimattrib(geoself(), \"hull_id\");
if (success) UpdateAttribute(\"hull_id\", \"max_hull_id\", i@primnum);"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
userdata	[ 0	locks=0 ]	(	0	)
opname	[ 0	locks=0	autoscope=1 ]	(	[ opname	update_B_attributes ] 	)
folder1	[ 0	locks=0 ]	(	0	0	)
}

