// VARIABLES
// ---------------------------------------------------------------------
string _opname = chs(\"opname\");
int success;

// ---------------------------------------------------------------------
// HELPERS
// ---------------------------------------------------------------------
#define PRINT(val) printf(\"%d\\n\", val)
#define WTF() printf(\"WTF? line: %d in: \\\"%s\\\"\\n\", __LINE__, _opname)
#define NOSUCCESS() printf(\"wasSuccess == %d in: \\\"%s\\\"\\n\", wasSuccess, _opname)

void UpdateAttributes(string attribname)
{
    int wasSuccess;
    int countA = detailattrib(1, attribname, 0, wasSuccess);
    int countB = detailattrib(2, attribname, 0, wasSuccess);
    
    wasSuccess = hasdetailattrib(geoself(), attribname);
    if (!wasSuccess) adddetailattrib(geoself(), attribname, 0);
    
    setdetailattrib(geoself(), attribname, countA + countB);
}

// ---------------------------------------------------------------------
// MAIN
// ---------------------------------------------------------------------

// update bundle_count
success = hasdetailattrib(@OpInput2, \"bundle_count\") && hasdetailattrib(@OpInput3, \"bundle_count\");
if (success) UpdateAttributes(\"bundle_count\");"	)
